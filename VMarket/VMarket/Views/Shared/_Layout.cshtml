<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - VMARKET</title>
    @Styles.Render("~/Content/css")
    
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>Simple Dashboard</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <meta name="layout" content="main" />

    <script type="text/javascript" src="http://www.google.com/jsapi"></script>

    <script src="../Content/js/jquery/jquery-1.8.2.min.js" type="text/javascript"></script>
    <link href="../Content/css/customize-template.css" type="text/css" media="screen, projection" rel="stylesheet" />

    <style>
        #body-content {
            padding-top: 40px;
        }
    </style>
</head>
<body>
    <div class="navbar navbar-fixed-top">
        <div class="navbar-inner">
            <div class="container">
                <button class="btn btn-navbar" data-toggle="collapse" data-target="#app-nav-top-bar">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a href="dashboard.html" class="brand"><i class="icon-leaf">Virtual Market</i></a>
                <div id="app-nav-top-bar" class="nav-collapse">
                    <ul class="nav">

                            <li>@Html.ActionLink("Home", "Index", "Home")</li>
                            <li>@Html.ActionLink("About", "About", "Home")</li>
                            <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                            <li>@Html.ActionLink("Departamento", "Index", "Departments")</li>
                            <li>@Html.ActionLink("Ciudad", "Index", "Cities")</li>
                            <li>@Html.ActionLink("Categria", "Index", "Categories")</li>
                            <li>@Html.ActionLink("Estado", "Index", "States")</li>
                            <li>@Html.ActionLink("Producto", "Index", "Products")</li>
                            <li>@Html.ActionLink("Empresa", "Index", "Companies")</li>
                    </ul>
                    @Html.Partial("_LoginPartial")
                </div>
            </div>
        </div>
    </div>

    <footer class="application-footer">
        <div class="container">
            <p>Application Footer</p>
            <div class="disclaimer">
                <p>This is an example disclaimer. All right reserved.</p>
                <p>Copyright © keaplogik 2011-2012</p>
            </div>
        </div>
    </footer>

    <script src="../Content/js/bootstrap/bootstrap-transition.js" type="text/javascript"></script>
    <script src="../Content/js/bootstrap/bootstrap-alert.js" type="text/javascript"></script>
    <script src="../Content/js/bootstrap/bootstrap-modal.js" type="text/javascript"></script>
    <script src="../Content/js/bootstrap/bootstrap-dropdown.js" type="text/javascript"></script>
    <script src="../Content/js/bootstrap/bootstrap-scrollspy.js" type="text/javascript"></script>
    <script src="../Content/js/bootstrap/bootstrap-tab.js" type="text/javascript"></script>
    <script src="../Content/js/bootstrap/bootstrap-tooltip.js" type="text/javascript"></script>
    <script src="../Content/js/bootstrap/bootstrap-popover.js" type="text/javascript"></script>
    <script src="../Content/js/bootstrap/bootstrap-button.js" type="text/javascript"></script>
    <script src="../Content/js/bootstrap/bootstrap-collapse.js" type="text/javascript"></script>
    <script src="../Content/js/bootstrap/bootstrap-carousel.js" type="text/javascript"></script>
    <script src="../Content/js/bootstrap/bootstrap-typeahead.js" type="text/javascript"></script>
    <script src="../Content/js/bootstrap/bootstrap-affix.js" type="text/javascript"></script>
    <script src="../Content/js/bootstrap/bootstrap-datepicker.js" type="text/javascript"></script>
    <script src="../Content/js/jquery/jquery-tablesorter.js" type="text/javascript"></script>
    <script src="../Content/js/jquery/jquery-chosen.js" type="text/javascript"></script>
    <script src="../Content/js/jquery/virtual-tour.js" type="text/javascript"></script>
    <script type="text/javascript">
        $(function() {
            $('#sample-table').tablesorter();
            $('#datepicker').datepicker();
            $(".chosen").chosen();
        });
    </script>





            <div class="container body-content">
                @RenderBody()
                <hr />
                <footer>
                    <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
                </footer>
            </div>

            @Scripts.Render("~/bundles/jquery")
            @Scripts.Render("~/bundles/bootstrap")
            @RenderSection("scripts", required: false)
</body>
</html>
